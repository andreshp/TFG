\begin{minted}[bgcolor=backg]{c}
LoadPackage("numericalsgps");
LoadPackage("FrancyMonoids");
s:=NumericalSemigroup(3,4,5);
gens:=s->Difference(MinimalGenerators(s),
                    [Multiplicity(s)]);
DrawTreeOfSonsOfNumericalSemigroup(s,5,gens);
\end{minted}

